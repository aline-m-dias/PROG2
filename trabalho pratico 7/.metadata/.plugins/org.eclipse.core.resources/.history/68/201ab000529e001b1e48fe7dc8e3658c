package principal;

import java.util.Scanner;

public class Main {

	public static void main(String [] args) {
		Scanner sc = new Scanner(System.in);
		double a=0, b=0;
		try {
			System.out.println("Digite o valor de a:");
			a= sc.nextDouble();
			System.out.println("Digite o valor de b:");
			b= sc.nextDouble();
			metodo1(a,b);
		}catch(ArithmeticException e) {
			System.out.println("Houve uma excessão");
			e.printStackTrace();
		}
		sc.close();
	}
	
	public static void metodo1(double a,double b) throws ArithmeticException{
		metodo2(a,b);
	}
	
	public static void metodo2(double a, double b) throws ArithmeticException {
		  double r=0;
		  r= a/b;
		 
	}
	
	
}
